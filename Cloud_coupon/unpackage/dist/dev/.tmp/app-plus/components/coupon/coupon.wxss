.coun_box{width: 100%;position: absolute;top: 00rpx;bottom: 20rpx;overflow: scroll;background: white;border-radius: 10rpx;padding-top: 20rpx;box-sizing: border-box;}
.user_image{width: 95%;margin: 0 2.5%;position: relative;color: white;height: 250rpx;margin-bottom: 40rpx;}
.user_image image{width: 100%;height: 100%;position: absolute;top: 0;left: 0;}
.user_le{position: absolute;left: 0;width: 70%;height: 100%;padding: 60rpx 0 60rpx 40rpx;box-sizing: border-box;}
.user_le view:nth-of-type(1){font-size: 1.4rem;font-weight: bold;margin-bottom: 40rpx;}
.user_le view:nth-of-type(2){font-size: 0.6rem;}
.user_ri{position: absolute;right: 0;width: 30%;height: 100%;font-size: 0.9rem;font-weight: bold;line-height: 250rpx;text-align: center;}

