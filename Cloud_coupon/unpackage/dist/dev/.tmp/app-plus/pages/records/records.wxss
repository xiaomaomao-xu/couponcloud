.trans_box{width: 100%;height: auto;overflow: hidden;background: #fbfbfb;padding: 30rpx 5%;-webkit-box-sizing: border-box;box-sizing: border-box;}
.trans_box>view{width: 100%;height: auto;overflow: hidden;}
.trans_time text{font-size: 0.9rem;font-weight: bold;display: block;float: left;}
.trans_time image{width: 40rpx;height: 40rpx;margin-top: 5rpx;display: block;float: left;}
.trans_money text{font-size: 0.7rem;color: #666666;margin-right: 10rpx;}
.follow_box{width: 100%;height: auto;overflow: hidden;background: white}
.post_titile{width: 90%;margin: 0 5%;height: auto;overflow: hidden;border-bottom: 1px solid #EAEAEA;}
.post_titile:last-child{border-bottom: none;}
.post_titile>image{width: 100rpx;height: 100rpx;float: left;border-radius: 50%;margin: 15rpx 0;}
.post_new{width: 85%;float: right;height: 130rpx;-webkit-box-sizing: border-box;box-sizing: border-box;padding: 0 15rpx;box-sizing: border-box;}
.new_le{width: 70%;float: left;height: auto;margin: 20rpx 0;}
.new_le view{width: 100%;float: left;overflow: hidden;white-space: nowrap;-o-text-overflow: ellipsis;text-overflow: ellipsis;}
.new_le view:nth-of-type(1){font-size: 0.9rem;font-weight: bold;}
.new_le view:nth-of-type(2){font-size: 0.7rem;color: #666666;}
.new_ri{font-size: 0.9rem;font-weight: bold;padding: 10rpx 20rpx;-webkit-box-sizing: border-box;box-sizing: border-box;float: right;margin: 28rpx 0;}

