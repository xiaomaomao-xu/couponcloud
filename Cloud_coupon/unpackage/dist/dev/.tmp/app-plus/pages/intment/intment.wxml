<view style="position:absolute;top:0;bottom:0;width:100%;overflow:scroll;background:#F0F0F0;"><view class="voucher_title"><block wx:for="{{titlebox}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['tapindex',[index]]]]]}}" bindtap="__e"><text class="{{[(titleindex===index)?'active':'']}}">{{item}}</text></view></block></view><scroll-view class="cart_list" style="height:100%;" scroll-y="true" data-event-opts="{{[['scrolltolower',[['lower',['$event']]]]]}}" bindscrolltolower="__e"><block wx:for="{{collelist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="dining_title"><view class="dining_pic"><block wx:if="{{https}}"><image src="{{item.pic}}"></image></block></view><view class="dining_text"><view class="text_list"><view>{{item.name}}</view><view><text>{{item.hour}}</text><view>:</view><text>{{item.minu}}</text><view>:</view><text>{{item.sec}}</text></view></view><view class="text_two">{{item.address}}</view><view class="text_three"><text>{{item.coupon}}</text></view><view class="text_five">{{'有效时间:'+item.orderid+"-"+item.warnremark+''}}</view></view></view></block><view hidden="{{!(defect_el)}}" class="defect"><block wx:if="{{https}}"><image src="{{https+'/front_image/fault.png'}}"></image></block><text defect_name="{{defect_name}}">{{defect_name}}</text></view></scroll-view></view>