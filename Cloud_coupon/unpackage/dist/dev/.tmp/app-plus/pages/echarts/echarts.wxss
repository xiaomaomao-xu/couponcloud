.time_box{width: 100%;height: 100rpx;padding: 20rpx 5%;box-sizing: border-box;background: white;}
.time_box input{width: 30%;height: 100%;float: left;border: 1px solid #535353;border-radius: 10rpx;font-size: 0.8rem;text-align: center;}
.time_box input:nth-of-type(2){margin-left: 5%;}
.time_box view{width: auto;font-size: 0.8rem;background: #fe6110;color: white;border-radius: 10rpx;float: right;padding: 0 20rpx;line-height: 60rpx;height: 100%;}
.pic_title{width: 100%;height: auto;padding: 0 5%;box-sizing: border-box;overflow: hidden;background: white;}
.pic_title view{display: -webkit-box;display: -webkit-flex;display: flex;width: 25%;height: 70rpx;line-height: 70rpx;float: left;}
.pic_title text:nth-of-type(1){width: 30rpx;height: 30rpx;border-radius: 50%;display: block;margin: 20rpx 10rpx;}
.pic_title text:nth-of-type(2){font-size: 0.8rem;}
.ac1{background: #fe6110;}
.ac2{background: #43a4f6;}
.ac3{background: #7943f6;}
.ac4{background: #ff0000;}
.ec-canvas {display: -webkit-box;display: -webkit-flex;display: flex;height: 100%;-webkit-box-flex: 1;-webkit-flex: 1;flex: 1;}
.canvasView {width: 700rpx;height: 500rpx;}
.data_list{width: 100%;height: auto;overflow: hidden;background: white;margin: 20rpx 0;font-size: 0.8rem;color: #000000;padding: 0 40rpx;box-sizing: border-box;}
.data_list>view{margin: 40rpx 0;}
.list_el{width: 100%;height: auto;overflow: hidden;}
.list_el view{width: 25%;height: auto;box-sizing: border-box;float: left;}
.list_el view:nth-of-type(1) text:nth-of-type(2){color: #fe6110;}
.list_el view:nth-of-type(2) text:nth-of-type(2){color: #43a4f6;}
.list_el view:nth-of-type(3) text:nth-of-type(2){color: #7943f6;}
.list_el view:nth-of-type(4) text:nth-of-type(2){color: #ff0000;}
.list_el view text{margin-left: 10rpx;}
.data_list:last-child{margin-bottom: 0 !important;}
.qiun-charts {
	width: 750rpx;
	height: 500rpx;
	background-color: #FFFFFF;
}
.charts {
	width: 750rpx;
	height: 500rpx;
	background-color: #FFFFFF;
}

