.center{width: 100%;background: #f0f0f0;height: auto;overflow: scroll;}
.center_bg{width: 100%;height: 400rpx;}
.center_bg image{width: 100%;height: 100%;}
.center_box{width: 90%;height: 400rpx;background: white;-webkit-box-shadow: 0 0 10rpx #EAEAEA;box-shadow: 0 0 10rpx #EAEAEA;border-radius: 10rpx;z-index: 999;margin: 0 5%;position: absolute;top: 200rpx;left: 0;}
.center_head{width: 100%;height: 140rpx;position: absolute;top: -70rpx;left: 0;}
.center_head image{width: 140rpx;height: 140rpx;border-radius: 50%;position: absolute;left: 50%;-webkit-transform: translateX(-50%);-ms-transform: translateX(-50%);transform: translateX(-50%);}
.center_mtion{width: 100%;height: auto;overflow: hidden;text-align: center;font-size: 0.8rem;}
.center_name{font-size: 1rem;font-weight: bold;margin-top: 70rpx;padding: 20rpx 0;}
.center_lable{font-size: 0.6rem;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-pack: center;-webkit-justify-content: center;-ms-flex-pack: center;justify-content: center;-webkit-box-align: center;-webkit-align-items: center;-ms-flex-align: center;align-items: center;}
.center_lable image{width: 30rpx;height: 30rpx;}
.center_number{width: 100%;height: auto;overflow: hidden;}
.number_list{width: 25%;height: auto;overflow: hidden;padding: 20rpx 0 20rpx 20rpx;-webkit-box-sizing: border-box;box-sizing: border-box;float: left;}
.number_list text{display: block;width: 100%;margin: 20rpx 0;text-align: center;font-size: 0.8rem;font-weight: bold;}
.number_list text:nth-of-type(1){font-size: 1.1rem;}
.myService{width: 90%;height: auto;overflow: hidden;margin: 40rpx 5%;border-radius: 10rpx;padding-top: 200rpx;}
.myService>view{background: white;}
.myService_title{font-size: 0.8rem;padding: 15rpx;border-bottom: 1px solid #EAEAEA;border-top-left-radius: 10rpx;border-top-right-radius: 10rpx;}
.myService_center{width: 100%;height: auto;overflow: scroll;}
.gopages{width: 25%;height: auto;overflow: hidden;float: left;-webkit-box-sizing: border-box;box-sizing: border-box;padding: 20rpx 0 20rpx 20rpx;}
.gopages image{width: 50rpx;height: 50rpx;display: block;}
.gopages view{width: 100%;font-size: 0.7rem;height: auto;overflow: hidden;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-pack: center;-webkit-justify-content: center;-ms-flex-pack: center;justify-content: center;-webkit-box-align: center;-webkit-align-items: center;-ms-flex-align: center;align-items: center;padding: 10rpx 0;}

