<view style="position:absolute;top:0;bottom:0;width:100%;left:0;background:white;height:auto;overflow:scroll;"><scroll-view class="cart_list" style="height:100%;" scroll-y="true" data-event-opts="{{[['scrolltolower',[['lower',['$event']]]]]}}" bindscrolltolower="__e"><block wx:for="{{collelist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="collect"><view class="collect_le"><block wx:if="{{https}}"><image src="{{https+'/'+item.film.filmPic}}"></image></block></view><view class="collect_ri"><view class="collect_add"><text>{{item.film.filmName}}</text><text>x1</text></view><view class="collect_sales">{{item.film.filmRemark}}</view><view class="collect_res">{{item.film.filmTime}}</view><view><text class="collect_time">{{item.ticket.ticketStarttime+" "+item.ticket.ticketStarthour}}</text><text data-event-opts="{{[['tap',[['film_el',[index]]]]]}}" class="collect_btn" bindtap="__e">去使用</text></view></view><view hidden="{{!(defect_el)}}" class="defect"><block wx:if="{{https}}"><image src="{{https+'/front_image/fault.png'}}"></image></block><text defect_name="{{defect_name}}">{{defect_name}}</text></view></view></block></scroll-view><view hidden="{{!(video)}}" class="video_el"></view><view hidden="{{!(video)}}" class="film_box"><image src="../../static/images/icon16.png" data-event-opts="{{[['tap',[['video_list',['$event']]]]]}}" bindtap="__e"></image><view class="film"><view>{{namelist.cinema.cinemaName}}</view><view>{{namelist.film.filmName}}</view><view>{{namelist.ticket.ticketStarttime+"   "+namelist.ticket.ticketStarthour+"-"+namelist.ticket.ticketEndhour+"   "+namelist.film.filmSortType}}</view><view>{{"电影票编号:"+namelist.ticket.ticketSerialId}}</view></view></view></view>