.center{width: 100%;background: #f0f0f0;position: absolute;top: 0;bottom: 0;}
.center>view{width: 90%;margin: 5%;background: white;border-radius: 10rpx;}
.business_top{height: 50%;padding: 40rpx 0;box-sizing: border-box;}
.business_bot{height: 40%;}
.business_top>view{width: 100%;padding: 10rpx 0;text-align: center;}
.business_top>view:nth-of-type(1){font-size: 1rem;color: #000000;font-weight: 600;}
.business_top>view:nth-of-type(2){font-size: 1.2rem;color: red;}
.business_top>view:nth-of-type(3){font-size: 0.8rem;color: #666666;}
.coupon_box{width: 100%;height: auto;overflow: hidden;margin: 40rpx 0;}
.coupon_box view{width: 33%;padding: 0 40rpx;box-sizing: border-box;border-left: 1px solid #EAEAEA;float: left;}
.coupon_box view:nth-of-type(1){border: none !important;}
.coupon_box view text{width: 100%;text-align: center;display: block;font-size: 0.9rem;color: #000000;padding: 5rpx 0;}
.coupon_box view text:nth-of-type(1){color: red;}
.coupon_btn{width: 90% !important;height: 100rpx;line-height: 100rpx;font-size: 0.9rem;color: white;font-weight: 400;margin: 0 5%;background: #f18711;padding: 0 !important;border-radius: 10rpx;}
.myService{width: 100%;height: auto;overflow: hidden;border-radius: 10rpx;}
.myService>view{background: white;}
.myService_center{width: 100%;height: auto;overflow: scroll;}
.gopages{width: 33%;height: auto;overflow: hidden;float: left;box-sizing: border-box;padding: 20rpx 0 20rpx 20rpx;}
.gopages image{width: 50rpx;height: 50rpx;display: block;}
.gopages view{width: 100%;font-size: 0.7rem;height: auto;overflow: hidden;display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: center;-webkit-justify-content: center;justify-content: center;-webkit-box-align: center;-webkit-align-items: center;align-items: center;padding: 10rpx 0;}

