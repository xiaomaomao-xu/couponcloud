.auth_pic{width: 95%;height: 600rpx;margin: 20rpx 2.5%;position: relative;}
.auth_pic image:nth-of-type(1){width: 100%;height: 100%;}
.auth_pic view{position: absolute;bottom: 20rpx;left: 20rpx;font-size: 0.7rem;color: white;}
.auth_pic image:nth-of-type(2){position: absolute;width: 30rpx;height: 30rpx;right: 20rpx;bottom: 20rpx;}
.btn_submit{width: 100%;height: auto;overflow: hidden;font-size: 0.9rem;color: white;background: white;position: fixed;bottom: 0rpx;left: 0%;border-top: 1px solid #EAEAEA;}
.btn_submit text{width: 60%;height: 100rpx;margin: 20rpx 20%;border-radius: 10rpx;text-align: center;background: #eb6100;display: block;line-height: 100rpx;}
.dining_ment{width: 100%;position: fixed;bottom: 0;height: 120rpx;font-size: 0.8rem;line-height: 120rpx;background:white;}
.dining_ment view:nth-of-type(1){padding: 0 40rpx;-webkit-box-sizing: border-box;box-sizing: border-box;width: 70%;float: left;color:red;font-weight:bold;font-size: 0.9rem;}
.dining_ment view:nth-of-type(2){text-align: center;float: right;width: 30%;float: right;background: #ee7d42;color: white;}
.dining_rule{width:90%;margin: 20rpx 5%;background: white;border-radius: 10rpx;height: auto;overflow: hidden;padding: 20rpx;-webkit-box-sizing: border-box;box-sizing: border-box;}
.dining_rule view:nth-of-type(1){font-size: 0.9rem;font-weight: bold;margin-bottom: 20rpx;}
.dining_rule view:nth-of-type(2){font-size: 0.7rem;color: #666666;word-wrap: break-word;word-break: normal;}
.busin_list{width: 90%;padding: 40rpx 20rpx;-webkit-box-sizing: border-box;box-sizing: border-box;margin: 40rpx 5%;height: auto;font-size: 0.8rem;font-weight: bold;background: white;border-radius: 10rpx;}
.busin_list text:nth-of-type(2){float: right;}

