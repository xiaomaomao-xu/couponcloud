.lssui_list{width: 100%;height: 120rpx;border-bottom: 1px solid #EAEAEA;line-height: 120rpx;font-size: 0.8rem;}
.lssui_box{width: 100%;height: auto;overflow: hidden;background: white;padding: 0 5%;-webkit-box-sizing: border-box;box-sizing: border-box;margin: 20rpx 0;}
.lssui_list >view{width: auto;float: left;height: 100%;}
.lssui_list input{height: 100%;width: auto;float: right !important;text-align: right;padding: 0 40rpx;}
.lssui_list image{width: 20rpx;height: 20rpx;float: right;margin: 50rpx 20rpx;}
.lssui_list:last-child{border-bottom: 0px !important;}
.list_oust{width: 100%;height: auto;overflow: hidden;}
.list_oust view{font-size: 0.8rem;text-align: right;width: 33%;height: 100rpx;border-bottom: 1px solid #EAEAEA;line-height: 100rpx;float: left;text-align: center;}
.active{background: rgba(0,0,0,0.1);}
.switch{float: right !important;}
.list_time{width: 60% !important;float: right !important;text-align: right;padding: 0 40rpx;}
.notice{margin: 20rpx 0;padding: 30rpx;-webkit-box-sizing: border-box;box-sizing: border-box;width: 100%;height: auto;overflow: hidden;background: white;}
.notice textarea{width: 100%;height: 400rpx;font-size: 0.8rem;}
.preview{width: 100%;height: 100rpx;background: white;position: fixed;bottom: 0rpx;left: 0;font-size: 1rem;}
.preview text:nth-of-type(1){display: block;float: left;width: 65%;color: red;text-align: right;padding: 0 40rpx;-webkit-box-sizing: border-box;box-sizing: border-box;line-height: 100rpx;height: 100%;}
.preview text:nth-of-type(2){display: block;width: 35%;height: 100%;text-align: center;color: white;background: #f78f0c;float: right;line-height: 100rpx;}
.preview_tiitle{width: 90%;height: 400rpx;margin: 40rpx 5%;position: relative;border-radius: 10rpx;overflow: hidden;}
.preview_tiitle image{position: absolute;top: 0;left: 0;width: 100%;height: 100%;}
.preview_tiitle view{position: absolute;left: 50rpx;top: 50rpx;z-index: 999;}
.preview_tiitle view text{display: block;margin-bottom: 40rpx;}
.preview_tiitle view text:nth-of-type(1){font-size: 1.3rem;color: #f0f0f0f0;}
.preview_tiitle view text:nth-of-type(2){font-size: 2.3rem;color: white;}
.preview_tiitle view text:nth-of-type(3){font-size: 0.7rem;color: white;}
.modol{padding: 20rpx 40rpx;-webkit-box-sizing: border-box;box-sizing: border-box;width: 100%;height: auto;overflow: hidden;font-size: 0.7rem;color: #666666;}

