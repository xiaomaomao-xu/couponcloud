.swiper1{height: 100rpx;overflow: hidden;}
.soon_swiper{background: white;z-index: 999;}
.item_el{text-align: center;font-size: 0.8rem;line-height: 100rpx;}
.film_box{width: 100%;position: absolute;top: 100rpx;bottom: 0;overflow: scroll;}
.film_list{width: 100%;height: auto;overflow: hidden;padding: 40rpx 2.5%;box-sizing: border-box;background: white;border-top: 1px solid #EAEAEA;z-index: 999;}
.list_el{width: 100%;height: auto;overflow: hidden;margin-bottom: 20rpx;}
.list_el text:nth-of-type(1){float: left;font-size: 0.9rem;font-weight: bold;}
.list_el text:nth-of-type(2){float: right;font-size: 0.8rem;color: #666666;}
.list_icon{width: 100%;font-size: 0.8rem;}
.introduce{position: absolute;top: 0;left: 0;bottom: 0;right: 0;}
.introduce image{width: 100%;height: 100%;-webkit-filter: blur(20rpx);filter: blur(20rpx);}
.mouclist{position: absolute;position: absolute;top: 0;left: 0;bottom: 0;right: 0;z-index: 999;}
.intrtitle{width: 100%;height: 400rpx;padding: 30rpx;box-sizing: border-box;}
.infinite_le{width: 35%;float: left;height: 100%;}
.infinite_le image{width: 100%;height: 100%;}
.infinite_ri{width: 65%;float: right;height: 100%;padding: 0 40rpx;box-sizing: border-box;}
.infinite_ri view{font-size: 0.8rem;color: white;}
.infinite_ri view:nth-of-type(1){font-weight: 600;font-size: 1rem;}
.box_view{width: 100%;position: absolute;top: 400rpx;bottom: 0;overflow: hidden;background: white;}
._h6{color: white;}
h8{color: #EAEAEA;display: block;width: 100%;font-size: 0.7rem;margin: 20rpx 0;}
h7{color: red;display: block;width: 100%;font-size: 0.9rem;margin: 20rpx 0 0 0;}
.active{border-bottom: 1px solid red;box-sizing: border-box;color: red;}

