.swiper1{height: 100rpx;overflow: hidden;}
.soon_swiper{background: white;z-index: 999;}
.item_el{text-align: center;font-size: 0.8rem;line-height: 100rpx;}
.film_box{width: 100%;height: auto;overflow: scroll;}
.film_list{width: 100%;height: auto;overflow: hidden;padding: 30rpx 2.5%;box-sizing: border-box;background: white;border-top: 1px solid #EAEAEA;z-index: 999;}
.list_el{width: auto;height: 100%;float: left;}
.list_el view:nth-of-type(1){font-size: 1rem;font-weight: bold;padding: 10rpx 0;box-sizing: border-box;}
.list_el view:nth-of-type(1) text:nth-of-type(2){font-size: 0.8rem;margin-left: 40rpx;}
.list_el view:nth-of-type(2){font-size: 0.7rem;color: #666666;padding: 10rpx 0;box-sizing: border-box;}
.list_el view:nth-of-type(2) text:nth-of-type(2){margin-left: 40rpx;}
.list_icon{font-size: 0.8rem;width: auto;padding: 10rpx 20rpx;margin: 40rpx 0;border: 1px solid red;color: red;border-radius: 5rpx;float: right;}
.active{border-bottom: 1px solid red;box-sizing: border-box;color: red;}
.cinema_title{width: 100%;height: auto;overflow: hidden;background: white;padding: 30rpx 2.5%;box-sizing: border-box;border-bottom: 1px solid #EAEAEA;border-top: 1px solid #EAEAEA;}
.cinema_title view:nth-of-type(1){font-size: 0.9rem;font-weight: bold;color: #000000;margin-bottom: 10rpx;}
.cinema_title view:nth-of-type(2){font-size: 0.7rem;color: #666666;}

